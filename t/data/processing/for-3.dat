    <t:if x=0>aa</t:if>
  </t:for>
  <t:attr name="'z'" value=9>
  xx
#output
<!DOCTYPE html><html><body>a<p x="1" v1="1" v2="2" v3="3" v4="4" z="9">xx</p></body></html>

