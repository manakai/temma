#data
a<p>
  <t:attr name="'x'" value="1">
  <t:for x=[1..10] as=x>
    <t:attr name="'v' . $x" value="$x">
    <t:if x="$x > 4"><t:last></t:if>
    <t:if x=0>aa</t:if>
  </t:for>
  <t:attr name="'z'" value=9>
  xx
#output
<!DOCTYPE html><html><body>a<p x="1" v1="1" v2="2" v3="3" v4="4" v5="5" z="9">xx</p></body></html>

#data
